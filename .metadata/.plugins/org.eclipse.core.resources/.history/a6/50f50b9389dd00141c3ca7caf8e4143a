package model;

import control.Car;
import greenfoot.World;

public class TestWorld extends World{
	
	public static int worldWidth = 800;
	public static int worldHeight = 1000;
	public static int cellSize = 1;
	
	public TestWorld() {
		super(worldHeight, worldWidth, cellSize);
		this.addObject(new Car(), 100, 100);
	}

}
